{
  "name": "functional-tables",
  "version": "1.0.0",
  "description": "HTML tables from functional programming",
  "scripts": {
    "clean": "rm -r dist",
    "start": "parcel __tests__/test-bulma.html",
    "test": "jest --coverage --collectCoverageFrom=src/**",
    "check": "rome check */*.ts --apply-suggested",
    "format": "rome format */*ts --write",
    "benchmark": "tsc --outDir dist benchmark/svd.ts && node dist/benchmark/svd.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/annolangen/functional-tables.git"
  },
  "author": "",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/annolangen/functional-tables/issues"
  },
  "homepage": "https://github.com/annolangen/functional-tables#readme",
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@types/benchmark": "^2.1.2",
    "@types/jest": "^29.2.4",
    "jest": "^29.3.1",
    "parcel": "^2.8.2",
    "prettier": "2.7.1",
    "rome": "^11.0.0",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "benny": "^3.7.1",
    "lit": "^2.2.1"
  }
